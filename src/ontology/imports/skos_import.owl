Ontology(<https://w3id.org/schulfach/sf/imports/skos_import.owl>
<https://w3id.org/schulfach/sf/releases/2025-10-20/imports/skos_import.owl>
Annotation(owl:versionInfo "2025-10-20")

Declaration(Class(<http://www.w3.org/2004/02/skos/core#Concept>))
Declaration(Class(<http://www.w3.org/2004/02/skos/core#ConceptScheme>))
Declaration(ObjectProperty(<http://www.w3.org/2004/02/skos/core#closeMatch>))
Declaration(ObjectProperty(<http://www.w3.org/2004/02/skos/core#exactMatch>))
Declaration(ObjectProperty(<http://www.w3.org/2004/02/skos/core#hasTopConcept>))
Declaration(ObjectProperty(<http://www.w3.org/2004/02/skos/core#topConceptOf>))
Declaration(AnnotationProperty(<http://purl.org/dc/terms/creator>))
Declaration(AnnotationProperty(<http://purl.org/dc/terms/description>))
